  rdfs:range  xsd:string .

mappdc:hasRRID a owl:DatatypeProperty ;
  rdfs:label "RRID"@en ;
  rdfs:domain prov:Entity ;
  rdfs:range  xsd:string .

mappdc:hasDoseValue a owl:DatatypeProperty ;
  rdfs:label "dose value"@en ;
  rdfs:domain mappdc:DosingRegimen ;
  rdfs:range  xsd:decimal .

mappdc:hasDoseUnit a owl:ObjectProperty ;
  rdfs:label "dose unit (SKOS concept or UO term)"@en ;
  rdfs:domain mappdc:DosingRegimen ;
  rdfs:range  skos:Concept .

mappdc:hasRoute a owl:ObjectProperty ;
  rdfs:label "route of administration (SKOS concept)"@en ;
  rdfs:domain mappdc:Intervention ;
  rdfs:range  skos:Concept .

mappdc:hasFrequencyPerDay a owl:DatatypeProperty ;
  rdfs:label "frequency per day"@en ;
  rdfs:domain mappdc:DosingRegimen ;
  rdfs:range  xsd:decimal .

### ---- Helpful alignments to DataCite (illustrative) ----
# Use these sparingly; prefer subPropertyOf to avoid over-commitment.

mappdc:describesStudy rdfs:subPropertyOf datacite:hasDescription .

### ---- Reuse patterns: Identifier, Title, Creators, Publisher, Description ----

### Identifier pattern (primary DOI + alternates)
# Attach identifiers to the DataCite resource describing your dataset/record.
# Use datacite:PrimaryResourceIdentifier for DOI, and datacite:AlternateResourceIdentifier for others.
# Values are carried by literal:hasLiteralValue; schemes by datacite:usesIdentifierScheme.

# Example (replace :dataset123 with your resource IRI)
# :dataset123
#   datacite:hasIdentifier [
#     a datacite:PrimaryResourceIdentifier ;
#     literal:hasLiteralValue "10.1234/abcd.efgh" ;
#     datacite:usesIdentifierScheme datacite:doi
#   ] ;
#   datacite:hasIdentifier [
#     a datacite:AlternateResourceIdentifier ;
#     literal:hasLiteralValue "https://example.org/landing/dataset123" ;
#     datacite:usesIdentifierScheme datacite:url
#   ] .

### Title pattern
# Use dcterms:title with language tags. If you need title types (e.g., AlternativeTitle),
# use mappdc:hasTitleType with a value from mappdc:TitleType (see vocab) when needed.

### Creator pattern
# Prefer ordered creators via datacite:hasCreatorList to a co:List of agents (schema:Person or schema:Organization).
# Each agent can carry an identifier via datacite:hasIdentifier â†’ datacite:PersonalIdentifier/OrganizationIdentifier
# with datacite:usesIdentifierScheme (e.g., datacite:orcid, datacite:ror) and literal:hasLiteralValue.

### Contributor pattern with roles (CRediT)
# Use dcterms:contributor to point to schema:Person or schema:Organization.
# Attach role(s) using prov:hadRole pointing to SKOS concepts in mappdc:CreditRoleScheme.
# Example:
# :dataset123 dcterms:contributor :alice .
# :alice a schema:Person ; schema:name "Alice Example" ;
#   prov:hadRole mappdc:roleInvestigation, mappdc:roleWritingOriginalDraft ;
#   datacite:hasIdentifier [ a datacite:PersonalIdentifier ; literal:hasLiteralValue "0000-0002-1825-0097" ; datacite:usesIdentifierScheme datacite:orcid ] .

### Publisher pattern
# Use dcterms:publisher pointing to a schema:Organization.
# If available, give the organization a ROR/ISNI using the same identifier pattern.

### Description pattern
# Use datacite:hasDescription to a node with one datacite:hasDescriptionType (e.g., datacite:abstract)
# and the free-text in literal:hasLiteralValue.

### Resource type pattern
# Use datacite:hasGeneralResourceType pointing to a skos:Concept (per DataCite list via dcmitype/fabio).

### Publication year pattern
# Use dcterms:issued with xsd:gYear (or schema:datePublished as an alternative).

### Subject pattern
# Use dcterms:subject to a skos:Concept in a scheme (e.g., MeSH, NCI Thesaurus); literals are acceptable when needed.

### Related identifier pattern (when not resolvable as a URI)
# Use mappdc:hasRelatedIdentifier to attach a related identifier node with value and scheme.

### ---- New terms to cover DataCite JSON gaps ----

mappdc:RelatedIdentifier a owl:Class ;
  rdfs:label "Related identifier"@en ;
  rdfs:subClassOf literal:Literal ;
  dcterms:description "Literal reification for a related identifier not modelled by SPAR/DataCite properties; carries a value and a scheme, and may be paired with a relation type elsewhere."@en .

mappdc:hasRelatedIdentifier a owl:ObjectProperty ;
  rdfs:label "has related identifier"@en ;
  rdfs:domain datacite:Resource ;
  rdfs:range  mappdc:RelatedIdentifier ;
  rdfs:comment "Attach a related identifier (non-primary) to a DataCite resource when only an identifier string is available."@en .

### Optional: title typing if needed
mappdc:hasTitleType a owl:ObjectProperty ;
  rdfs:label "has title type"@en ;
  rdfs:domain datacite:Resource ;
  rdfs:range  skos:Concept ;
  rdfs:comment "Classify titles (e.g., AlternativeTitle, Subtitle) using a SKOS concept from mappdc:TitleType."@en .

### ---- Preclinical genetics and pharmacology additions ----

# Species on study (or dataset) as a SKOS concept (e.g., NCBITaxon)
mappdc:hasSpecies a owl:ObjectProperty ;
  rdfs:label "has species"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  skos:Concept ;
  rdfs:comment "Primary species studied; prefer SKOS concepts (e.g., NCBITaxon)."@en .

# Gene entity and properties (lightweight when external IDs are not provided)
mappdc:Gene a owl:Class ;
  rdfs:label "Gene"@en ;
  rdfs:subClassOf prov:Entity .

mappdc:hasGene a owl:ObjectProperty ;
  rdfs:label "has gene"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  mappdc:Gene .

mappdc:geneSymbol a owl:DatatypeProperty ;
  rdfs:label "gene symbol"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

mappdc:geneName a owl:DatatypeProperty ;
  rdfs:label "gene name"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

mappdc:geneAccessionId a owl:DatatypeProperty ;
  rdfs:label "gene accession identifier"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

mappdc:geneChromosomeLocation a owl:DatatypeProperty ;
  rdfs:label "gene chromosome location"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

mappdc:geneMutationType a owl:ObjectProperty ;
  rdfs:label "gene mutation type"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  skos:Concept ;
  rdfs:comment "Use SO terms when possible; otherwise a SKOS concept from mappdc vocab."@en .

mappdc:alleleAccessionId a owl:DatatypeProperty ;
  rdfs:label "allele accession identifier"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

mappdc:alleleSymbol a owl:DatatypeProperty ;
  rdfs:label "allele symbol"@en ;
  rdfs:domain mappdc:Gene ;
  rdfs:range  xsd:string .

# Pharmacological treatment fields mapped to Intervention
mappdc:treatmentName a owl:DatatypeProperty ;
  rdfs:label "treatment name"@en ;
  rdfs:domain mappdc:Intervention ;
  rdfs:range  xsd:string .

mappdc:treatmentProtocol a owl:DatatypeProperty ;
  rdfs:label "treatment protocol"@en ;
