# Related item (richer than related identifier)
mappdc:RelatedItem a owl:Class ;
  rdfs:label "Related item"@en ;
  dcterms:description "Structured related resource with relation type, identifiers, title, and type."@en .

mappdc:hasRelatedItem a owl:ObjectProperty ;
  rdfs:label "has related item"@en ;
  rdfs:domain datacite:Resource ;
  rdfs:range  mappdc:RelatedItem .

mappdc:relationType a owl:ObjectProperty ;
  rdfs:label "relation type"@en ;
  rdfs:domain mappdc:RelatedItem ;
  rdfs:range  skos:Concept ;
  rdfs:comment "Use concepts from mappdc:RelationTypeScheme (DataCite relationType list)."@en .

mappdc:relatedItemType a owl:ObjectProperty ;
