### ---- Funding, Rights, Language, Geo, RelatedItem, Dates, Size/Format/Version ----

# Funding reference
mappdc:FundingReference a owl:Class ;
  rdfs:label "Funding reference"@en ;
  dcterms:description "Funding statement including funder, award number/title/URI."@en .

mappdc:hasFunding a owl:ObjectProperty ;
  rdfs:label "has funding"@en ;
  rdfs:domain datacite:Resource ;
  rdfs:range  mappdc:FundingReference .

mappdc:funder a owl:ObjectProperty ;
  rdfs:label "funder"@en ;
  rdfs:domain mappdc:FundingReference ;
  rdfs:range  schema:Organization ;
  rdfs:comment "Funder organization; identify with datacite:FunderIdentifier (e.g., datacite:fundref) when possible."@en .

mappdc:awardNumber a owl:DatatypeProperty ;
  rdfs:label "award number"@en ;
  rdfs:domain mappdc:FundingReference ;
  rdfs:range  xsd:string .

mappdc:awardTitle a owl:DatatypeProperty ;
  rdfs:label "award title"@en ;
  rdfs:domain mappdc:FundingReference ;
  rdfs:range  xsd:string .

mappdc:awardURI a owl:DatatypeProperty ;
  rdfs:label "award URI"@en ;
  rdfs:domain mappdc:FundingReference ;
  rdfs:range  xsd:anyURI .

