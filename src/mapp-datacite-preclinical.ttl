    rdfs:comment "Extension of SPAR/DataCite for preclinical research datasets, studies, subjects, interventions, endpoints, and sessions."@en .

### ---- Core classes (add more as needed) ----

mappdc:PreclinicalStudy a owl:Class ;
  rdfs:label "Preclinical study"@en ;
  rdfs:subClassOf prov:Activity ;
  dcterms:description "A structured preclinical experiment producing one or more datasets and outputs."@en .

mappdc:Subject a owl:Class ;
  rdfs:label "Subject (animal)"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "An experimental animal subject (e.g., a mouse or rat) in a preclinical study."@en .

mappdc:Group a owl:Class ;
  rdfs:label "Group/Cohort"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "A set of subjects sharing experimental conditions (e.g., treatment vs control)."@en .

mappdc:Intervention a owl:Class ;
  rdfs:label "Intervention"@en ;
  rdfs:subClassOf prov:Activity ;
  dcterms:description "An administered treatment or manipulation (compound, dose, route, schedule)."@en .

mappdc:DosingRegimen a owl:Class ;
  rdfs:label "Dosing regimen"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "Specification of dose amount, unit, frequency, and duration."@en .

mappdc:Endpoint a owl:Class ;
  rdfs:label "Endpoint"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "A measured outcome (e.g., activity, food intake, ECG metric)."@en .

mappdc:RecordingSession a owl:Class ;
  rdfs:label "Recording session"@en ;
  rdfs:subClassOf prov:Activity ;
  dcterms:description "A data acquisition session (time-bounded) producing files linked to a dataset/distribution."@en .

mappdc:HCMSystem a owl:Class ;
  rdfs:label "Home cage monitoring system"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "Hardware/software stack used for continuous behavioral/physiological monitoring in the home cage."@en .

mappdc:EthicalApproval a owl:Class ;
  rdfs:label "Ethical approval"@en ;
  rdfs:subClassOf prov:Entity ;
  dcterms:description "Ethical authorization (IACUC/CEEA) for procedures performed in the study."@en .

### ---- Object properties ----

mappdc:describesStudy a owl:ObjectProperty ;
  rdfs:label "describes study"@en ;
  rdfs:comment "Links a DataCite resource/identifier to the preclinical study it documents."@en ;
  rdfs:domain datacite:Resource ;
  rdfs:range  mappdc:PreclinicalStudy .

mappdc:hasSubject a owl:ObjectProperty ;
  rdfs:label "has subject"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  mappdc:Subject .

mappdc:inGroup a owl:ObjectProperty ;
  rdfs:label "in group"@en ;
  rdfs:domain mappdc:Subject ;
  rdfs:range  mappdc:Group .

mappdc:receivedIntervention a owl:ObjectProperty ;
  rdfs:label "received intervention"@en ;
  rdfs:domain mappdc:Subject ;
  rdfs:range  mappdc:Intervention .

mappdc:hasDosingRegimen a owl:ObjectProperty ;
  rdfs:label "has dosing regimen"@en ;
  rdfs:domain mappdc:Intervention ;
  rdfs:range  mappdc:DosingRegimen .

mappdc:hasEndpoint a owl:ObjectProperty ;
  rdfs:label "has endpoint"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  mappdc:Endpoint .

mappdc:hasRecordingSession a owl:ObjectProperty ;
  rdfs:label "has recording session"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  mappdc:RecordingSession .

mappdc:usesHCMSystem a owl:ObjectProperty ;
  rdfs:label "uses HCM system"@en ;
  rdfs:domain mappdc:RecordingSession ;
  rdfs:range  mappdc:HCMSystem .

mappdc:hasEthicalApproval a owl:ObjectProperty ;
  rdfs:label "has ethical approval"@en ;
  rdfs:domain mappdc:PreclinicalStudy ;
  rdfs:range  mappdc:EthicalApproval .

