@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix pcdo: <https://w3id.org/preclindo/ontology/pcdo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

pcdo:AdministrationRouteScheme a skos:ConceptScheme ;
  dcterms:title "PCDO Administration Routes"@en ;
  dcterms:description "A minimal SKOS scheme of common administration routes for preclinical interventions."@en .

pcdo:Route_IP a skos:Concept ;
  skos:inScheme pcdo:AdministrationRouteScheme ;
  skos:prefLabel "intraperitoneal"@en ;
  skos:altLabel "i.p."@en .

pcdo:Route_IV a skos:Concept ;
  skos:inScheme pcdo:AdministrationRouteScheme ;
  skos:prefLabel "intravenous"@en ;
  skos:altLabel "i.v."@en .

pcdo:Route_PO a skos:Concept ;
  skos:inScheme pcdo:AdministrationRouteScheme ;
  skos:prefLabel "per os (oral)"@en ;
  skos:altLabel "p.o."@en .

